<?xml version="1.0" encoding="utf-8"?>
<com.anagaf.freqhelper.RangeTableRow
    xmlns:a="http://schemas.android.com/apk/res/android"
    a:layout_width="match_parent"
    a:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        a:id="@+id/title"
        style="@style/RangeTitle"
        a:text="LPD69"
        a:layout_marginBottom="@dimen/gap"
        />

    <Button
        a:id="@+id/prev_channel_button"
        style="@style/ChannelButtonPrev"/>

    <EditText
        a:id="@+id/channel"
        style="@style/ChannelEdit"
        tools:text="5"/>

    <Button
        a:id="@+id/next_channel_button"
        style="@style/ChannelButtonNext"/>

</com.anagaf.freqhelper.RangeTableRow>